{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "nwintech-adf"
		},
		"AzureDataLakeStorageLS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorageLS'"
		},
		"AzureDataLakeStorageLS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://nwintechsa.dfs.core.windows.net/"
		},
		"AzureDatabricksLS_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0414-044916-m5wc0l"
		},
		"AzureKeyVault1_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://nwintechkv.vault.azure.net/"
		},
		"SqlServerOnPremLS_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-LGEL5NK\\SQLEXPRESS"
		},
		"SqlServerOnPremLS_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksLT2022"
		},
		"SqlServerOnPremLS_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "luke"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "nwintech-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageLS_accountKey": {
						"value": "[parameters('AzureDataLakeStorageLS_accountKey')]"
					},
					"AzureDataLakeStorageLS_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageLS_properties_typeProperties_url')]"
					},
					"AzureKeyVault1_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault1_properties_typeProperties_baseUrl')]"
					},
					"AzureDatabricksLS_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricksLS_properties_typeProperties_existingClusterId')]"
					},
					"SqlServerOnPremLS_properties_typeProperties_server": {
						"value": "[parameters('SqlServerOnPremLS_properties_typeProperties_server')]"
					},
					"SqlServerOnPremLS_properties_typeProperties_database": {
						"value": "[parameters('SqlServerOnPremLS_properties_typeProperties_database')]"
					},
					"SqlServerOnPremLS_properties_typeProperties_userName": {
						"value": "[parameters('SqlServerOnPremLS_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}